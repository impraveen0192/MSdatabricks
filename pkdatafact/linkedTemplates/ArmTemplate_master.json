{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "pkdatafact"
		},
		"AmazonS3_source_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'AmazonS3_source'"
		},
		"AzBlobStorage_Staging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzBlobStorage_Staging'"
		},
		"AzureBlob_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"AzureBlob_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "blob1"
		},
		"AmazonS3_source_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAZ5Z44B5EZNFKVZB4"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AmazonS3_source_secretAccessKey": {
						"value": "[parameters('AmazonS3_source_secretAccessKey')]"
					},
					"AzBlobStorage_Staging_connectionString": {
						"value": "[parameters('AzBlobStorage_Staging_connectionString')]"
					},
					"AmazonS3_source_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS3_source_properties_typeProperties_accessKeyId')]"
					},
					"AzureBlob_properties_typeProperties_fileName": {
						"value": "[parameters('AzureBlob_properties_typeProperties_fileName')]"
					},
					"AzureBlob_properties_typeProperties_folderPath": {
						"value": "[parameters('AzureBlob_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}